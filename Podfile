# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'ChatApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ChatApp

  # Firebase
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'Firebase/Storage'

  # login google , facebook
pod 'FBSDKLoginKit'
pod 'GoogleSignIn'

  # Message layout
pod 'MessageKit'

  # Progress
pod 'JGProgressHUD'

  # Local database
pod 'RealmSwift'

  # Load image internet
pod 'SDWebImage'
pod 'Kingfisher'


end
